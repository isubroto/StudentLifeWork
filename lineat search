#include<iostream>
using namespace std;
int main()
{
    int arr[10];
    int s;
    for(int i=0;i<10;i++)
    {
        cin >> arr[i];
    }
    cout<<"Input the number you want to search ";
    cin>>s;
    for(int i=0;i<10;i++)
    {
        if(arr[i] == s)
        {
            cout<<"The value is in index of"<<i<<endl;
        }
        else
            continue;
    }
}
